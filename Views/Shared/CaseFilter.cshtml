@model WorkFlowResponseModel   
<div class="modal fade show d-block " id="divModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" id="divSchedulerModal" role="document" >
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Add Proceeding - GTST</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="$('#divSchedulerModal').hide();$('#divModal').removeClass('d-block');">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="dialg" title="Definition: Knowledge">
                    <div style="width: 65%; float: left;">
                        <table style="margin: 0 auto;" class="tableborder">
                            <tr>
                                <td>                    
                                    Proceeding Name :                              
                                </td>
                                <td>                    
                                    <input type="text" value="Record Genetic Test Results">                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Proceeding Type :                          
                                </td>
                                <td>                    
                                    <input type="text" value="">                              
                                </td>
                            </tr>
                            <tr>
                                <td>                    
                                    Scheduling Unit :                       
                                </td>
                                <td>                    
                                    <input type="text" value="">                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Case ID :                               
                                </td>
                                <td>                    
                                    <input type="text" value="100016">                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    File Number :                           
                                </td>
                                <td>                    
                                    <input type="text" value="UR0017811" >                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Petition Number : 
                                </td>
                                <td>                    
                                    <input type="text" value="" >                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Event Start Date :                        
                                </td>
                                <td>                    
                                    <input type="text" value=@DateTime.Now.Date.AddDays(1)>                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Start Time :                    
                                </td>
                                <td>                    
                                    <input type="text" value="7:30 AM">                              
                                </td>
                            </tr>
                            <tr>
                                <td>                    
                                    End Time :                        
                                </td>
                                <td>                    
                                    <input type="text" value="8:30 AM">                              
                                </td>
                            </tr>
                            <tr>
                                <td>                    
                                    Offciating ID/Officer Code :                              
                                </td>
                                <td>                    
                                    <input type="text" value="">                              
                                </td>
                            </tr>
                            <tr>
                                <td>                    
                                    Location :                            
                                </td>
                                <td>                    
                                    <input type="text" value="">                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Notes :                          
                                </td> 
                                <td>                    
                                    <input type="text" value="">                              
                                </td> 
                            </tr>                                     
                        </table>
                    </div>
                    <div style="margin-left: 65%;">
                        <div>                            
                            <br />
                            MemberId : @Model.GTSTScheduler_Model?.MemberId <br />
                            Member Name : @Model.GTSTScheduler_Model?.LastName, @Model.GTSTScheduler_Model?.FirstName <br />                       
                            <br/>                            
                            <div class="button">
                                <a class="nav-link text-white" asp-area="" asp-controller="GTSTScheduler" asp-action="Save" asp-route-wfResponseModel=@Model>Accept Scheduling</a>                               
                            </div>                                    
                            <div>
                                <div class="button">
                                    <a class="nav-link text-white" href="#" onclick="$('#divSchedulerModal').hide();$('#divModal').removeClass('d-block');">Close</a>                               
                                </div>                                                   
                            </div>                                                                
                        </div>
                    </div>                    
            </div>            
        </div>
    </div>
</div>