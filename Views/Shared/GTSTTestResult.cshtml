@model WorkFlowResponseModel   
<div class="modal fade show d-block " id="divModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" id="divSchedulerModal" role="document" style="max-width: 700px !important;" >
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">GTST Test Result</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="$('#divSchedulerModal').hide();$('#divModal').removeClass('d-block');">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <br />
            <div>
                <div style="float:right">                                                                     
                    <div class="button">
                        <a class="nav-link text-white" asp-area="" asp-controller="NoticeGeneration" asp-action="Save" asp-route-processId=@Model.ProcessId asp-route-caseId=@Model.CaseDetailsModel?.CaseId>Save Results</a>                               
                    </div>                                                                                                                      
                </div>
            </div>
            <div class="modal-body">
                <div id="dialg" title="Definition: Knowledge">
                    <div>
                        <table style="margin: 0 auto;" class="tableborder">                            
                             <tr>
                                <td>                    
                                    Case ID :                               
                                </td>
                                <td>                    
                                    <input type="text" value=@Model.CaseDetailsModel?.CaseId>                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    File Number :                           
                                </td>
                                <td>                    
                                    <input type="text" value="UR0017811" >                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Result Date :                        
                                </td>
                                <td>                    
                                    <input type="text" value=@DateTime.Now.Date.AddDays(1)>                              
                                </td>
                            </tr>                            
                            <tr>
                                <td>                    
                                    Offciating ID/Officer Code :                              
                                </td>
                                <td>                    
                                    <input type="text" value="">                              
                                </td>
                            </tr>
                            <tr>
                                <td>                    
                                    Location :                            
                                </td>
                                <td>                    
                                    <input type="text" value="">                              
                                </td>
                            </tr>
                             <tr>
                                <td>                    
                                    Test Result :                          
                                </td> 
                                <td>                    
                                    <textarea rows="5" cols="60">
                                    </textarea>                            
                                </td> 
                            </tr>   
                            <tr>
                                <td>                    
                                    Notes :                          
                                </td> 
                                <td>                    
                                    <textarea rows="5" cols="60">
                                    </textarea>                            
                                </td> 
                            </tr>   
                             <tr>
                                <td>
                                    <br />
                                    <br />                                                                 
                                </td>
                            </tr> 
                        </table>
                    </div>                                     
                </div>
            </div>             
        </div>
    </div>
</div>